<!--pages/map/map.wxml-->
<import src="../pages/common/head.wxml" />
<view class="container">
  <template is="head" data="{{title: 'map'}}"/>
  <view>
    <view>
      <map
        style="width: 100%; height: 300px;"
        latitude="{{latitude}}"
        longitude="{{longitude}}"
        markers="{{markers}}"
        >
      </map>
    </view>
  </view>
</view>